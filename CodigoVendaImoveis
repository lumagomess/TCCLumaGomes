SELECT
  id,
  created_on,
  operation,
  property_type,
  place_name,
  country_name,
  state_name,
  price,
  currency,
  price_aprox_local_currency,
  price_aprox_usd,
  surface_total_in_m2,
  surface_covered_in_m2,
  price_usd_per_m2,
  price_per_m2,
  floor,
  rooms,
  expenses,
  place_with_parent_names
FROM
  properati-data-public.properties_br.properties_sell_201601
UNION ALL
